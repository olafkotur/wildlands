[file_customization]

folder_colors={
"res://scenes/entities/": "purple",
"res://scenes/interface/": "orange",
"res://scenes/main/": "green"
}

[global_group]

resource=""
player=""

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
